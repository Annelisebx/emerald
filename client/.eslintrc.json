{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "class-methods-use-this": 0,
        "no-else-return": 0,
        "react/jsx-filename-extension": 0,
        "object-curly-newline": 0,
        "no-unused-vars": ["warn"],
        "no-trailing-spaces": ["warn"],
        "react/jsx-indent": ["warn"]
    }
}
